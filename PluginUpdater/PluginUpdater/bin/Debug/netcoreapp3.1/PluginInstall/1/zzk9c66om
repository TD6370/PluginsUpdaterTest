	<script type="text/javascript">
	var online_image_url = '/thumb/zzk9c66om';
	var full_image_url = '';
	var is_mobile = '';
	var is_guest = true;
	var is_rtl = window.lang == 'ar' ? true : false;

	$(document).ready(function() {
		//online_image_url = online_image_url.replace('%SIZE%', (is_mobile ? '416x234' : '640x360'));
		//$('#picture_container').html('<a href="' + full_image_url + '"><img src="'+online_image_url + '"></a>');

		if (!is_mobile && !is_guest) {
			var setPictureContainerWidth = function() {
				var doc_width = document.documentElement.clientWidth,
					main_width = $('body #main').width(),
					margin = Math.ceil((doc_width-main_width)/2);
					
				if (doc_width <= main_width) {
					$('#picture_container').css({width: 'auto', marginLeft: 0, marginRight: 0});
				} else if (is_rtl) {
					$('#picture_container').css({width: doc_width, marginRight: '-' + margin + 'px'});
				} else {
					$('#picture_container').css({width: doc_width, marginLeft: '-' + margin + 'px'});
				}
			};
			setPictureContainerWidth();

			$(window).bind('resize', function() {
				setPictureContainerWidth();
			});
		}
		
		$.get(online_image_url, function(data) {
			$('#picture_container').html(data);
			$('#picture_container').addClass('active');
			
			if ($(data).hasClass('preview_error')) {
				if ($(data).is(':empty')) {
					$('#picture_container').removeClass('active');
				}
				return ;
			}
			$('.picture_preview .preview_text').show();
			
			if (is_guest) {
				$('#picture_container a img').click(function(evt) {
					evt.preventDefault();
					$('#free_btn').click();
					$.get('/c.php?a=gatewaypreviewclick');
				});
			} else {
				if (!is_mobile) {
					setPictureContainerWidth();
				}
				$('#picture_container>div>a').attr('href', $('a.df_button_lines:first').attr('href'));
			}
		});
		return false;	
	});

	</script>				        
